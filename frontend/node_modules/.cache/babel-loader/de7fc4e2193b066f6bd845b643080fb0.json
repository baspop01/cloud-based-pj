     this.selectSeats.push(this.seat[i].no_of_seats);\n      }\n\n      var select = this.seat[this.selectSeats.indexOf(r + c)];\n\n      if (select.selectSeat == false || select.selectSeat == undefined) {\n        select.selectSeat = true;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4EA;AACA;AACAA,aADA;AAEAC,MAFA,kBAEA;AACA;AACAC,cADA;AAEAC,eAFA;AAGAC,kBAHA;AAIAC,cAJA;AAKAC,cALA;AAMAC;AANA;AAQA,GAXA;AAYAC,SAZA,qBAYA;AACA;AACA,GAdA;AAeAC;AACAC,WADA,mBACAC,EADA,EACA;AAAA;;AACAC,YACAC,GADA,uCACAF,EADA,GAEAG,IAFA,CAEA;AACA;AACA;;AACA;;AACA;;AACAC;AACA,gCACAA,uDADA;;AAEA;AACA;AACA;AACA,SAHA,MAGA;AACA;AACA;AACA;AACA,OAjBA,EAkBAC,KAlBA,CAkBA;AACAC;AACA,OApBA;AAqBA,KAvBA;AAwBAC,eAxBA,uBAwBAC,IAxBA,EAwBA;AACA;AACA;AACA;AACA,KA5BA;AA6BAC,cA7BA,sBA6BAC,CA7BA,EA6BAC,CA7BA,EA6BA;AACA;AACA;AACA;;AACA;;AACA;AACAC;AACA;AACA;AArCA;AAfA","names":["name","data","seat","movie","seatBook","rows","cols","selectSeats","mounted","methods","getSeat","id","axios","get","then","date","catch","console","getSeatBook","Book","selectSeat","r","c","select"],"sourceRoot":"src/views","sources":["movie.vue"],"sourcesContent":["<template>\r\n  <section class=\"section\" id=\"app\">\r\n    <div class=\"content\">\r\n      <div class=\"columns\">\r\n        <div class=\"column\">\r\n          <img\r\n            style=\"\r\n              width: 232px;\r\n              height: 367px;\r\n              margin-left: 15%;\r\n              margin-right: 15%;\r\n            \"\r\n            src=\"https://s359.kapook.com/pagebuilder/cf0dd339-b4a5-49ef-8332-25d490e03da8.jpg\"\r\n          />\r\n        </div>\r\n        <div class=\"column is-6 level-right\">\r\n          <div class=\"title\">\r\n            <p class=\"title has-text-white\">{{ movie.m_name }}</p>\r\n          </div>\r\n          <hr />\r\n          <div class=\"detail\">\r\n            <p class=\"has-text-white has-text-justified\">\r\n              {{ movie.plot }}\r\n            </p>\r\n            <table\r\n              class=\"table is-bordered has-background-black has-text-white\"\r\n            >\r\n              <tr>\r\n                <td>Theatre</td>\r\n                <td>{{ movie.tname }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Date</td>\r\n                <td>{{ movie.show_date }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Time</td>\r\n                <td>{{ movie.st_time }} - {{ movie.end_time }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Director</td>\r\n                <td>{{ movie.director }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Cast</td>\r\n                <td>\r\n                  {{ movie.actors }}\r\n                </td>\r\n              </tr>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <table class=\"mx-auto\" style=\"width: 50%\">\r\n        <tbody>\r\n          <tr v-for=\"r in rows\" :key=\"r\">\r\n            <td v-for=\"c in cols\" class=\"pl-2\" style=\"width: 50px\" :key=\"c\">\r\n              <div class=\"has-background-success has-text-white\" v-if=\"seatBook.includes(r + c)\">\r\n                {{ r }}{{ c }}\r\n              </div>\r\n              <!-- <a v-else-if=\"seat.selectSeat = true\" ><div class=\"has-background-danger-dark has-text-white\">\r\n                {{ r }}{{ c }}\r\n              </div></a> -->\r\n              <a @click=\"selectSeat(r,c)\" v-else ><div class=\"has-background-danger has-text-white\">\r\n                {{ r }}{{ c }}\r\n              </div></a>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <div class=\"columns\" v-for=\"seat in seat\" :key=\"seat.seat_id\"></div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  data() {\r\n    return {\r\n      seat: [],\r\n      movie: [],\r\n      seatBook: [],\r\n      rows: [],\r\n      cols: [],\r\n      selectSeats: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getSeat(this.$route.params.id);\r\n  },\r\n  methods: {\r\n    getSeat(id) {\r\n      axios\r\n        .get(`http://localhost:3000/movie/${id}`)\r\n        .then((response) => {\r\n          this.seat = response.data.seat;\r\n          this.movie = response.data.movie[0];\r\n          this.getSeatBook(response.data.seatBook);\r\n          var date = this.movie.show_date.split(\"T\")[0];\r\n          date = date.split(\"-\");\r\n          this.movie.show_date =\r\n            date[0] + \"-\" + date[1] + \"-\" + (parseInt(date[2]) + 1);\r\n          if (this.seat[0].THEATRE_tid == 1 || this.seat[0].THEATRE_tid == 2) {\r\n            this.rows = [\"A\", \"B\", \"C\", \"D\", \"E\"];\r\n            this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\", \"06\", \"07\", \"08\", \"09\", \"10\"];\r\n          } else if (this.seat[0].THEATRE_tid == 3) {\r\n            this.rows = [\"A\", \"B\", \"C\"];\r\n            this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\"];\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    getSeatBook(Book) {\r\n      for (var i = 0; i < Book.length; i++) {\r\n        this.seatBook.push(Book[i].no_of_seats);\r\n      }\r\n    },\r\n    selectSeat(r, c){\r\n      for (var i = 0; i < this.seat.length; i++) {\r\n            this.selectSeats.push(this.seat[i].no_of_seats);\r\n      }\r\n      var select = this.seat[this.selectSeats.indexOf(r+c)]\r\n      if (select.selectSeat == false || select.selectSeat == undefined){\r\n        select.selectSeat = true\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}