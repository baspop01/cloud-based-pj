{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.search.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from '@/plugins/axios';\nexport default {\n  name: \"app\",\n  props: ['user'],\n  data: function data() {\n    return {\n      search: \"\",\n      movie: [],\n      order: \"show_date\",\n      theatre: \"none\"\n    };\n  },\n  mounted: function mounted() {\n    window.scrollTo(0, 0);\n    this.getMovie();\n  },\n  methods: {\n    getMovie: function getMovie() {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/\", {\n        params: {\n          search: this.search,\n          order: this.order,\n          theatre: this.theatre\n        }\n      }).then(function (response) {\n        _this.movie = response.data;\n\n        for (var i = 0; i < _this.movie.length; i++) {\n          _this.movie[i].show_date = new Date(_this.movie[i].show_date).toDateString();\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    image: function image(file_path) {\n      if (file_path) {\n        return \"http://localhost:3000/\" + file_path;\n      } else {\n        return \"https://bulma.io/images/placeholders/640x360.png\";\n      }\n    },\n    isAdmin: function isAdmin() {\n      if (!this.user) return false;\n      return this.user.role == 'admin';\n    },\n    isCusto: function isCusto() {\n      if (!this.user) return false;\n      return this.user.role == 'customer';\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuGA;AACA;AACAA,aADA;AAEAC,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,gBADA;AAEAC,eAFA;AAGAC,wBAHA;AAIAC;AAJA;AAMA,GAVA;AAWAC,SAXA,qBAWA;AACAC;AACA;AACA,GAdA;AAeAC;AACAC,YADA,sBACA;AAAA;;AACAC,YACAC,GADA,CACA,wBADA,EACA;AACAC;AACAV,6BADA;AAEAE,2BAFA;AAGAC;AAHA;AADA,OADA,EAQAQ,IARA,CAQA;AACA;;AACA;AACA;AACA;AACA,OAbA,EAcAC,KAdA,CAcA;AACAC;AACA,OAhBA;AAiBA,KAnBA;AAoBAC,SApBA,iBAoBAC,SApBA,EAoBA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AACA,KA1BA;AA2BAC,WA3BA,qBA2BA;AACA;AACA;AACA,KA9BA;AA+BAC,WA/BA,qBA+BA;AACA;AACA;AACA;AAlCA;AAfA","names":["name","props","data","search","movie","order","theatre","mounted","window","methods","getMovie","axios","get","params","then","catch","console","image","file_path","isAdmin","isCusto"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"container is-widescreen\">\r\n    <div class=\"card-image pt-5\">\r\n      <figure class=\"image is-1186x800\">\r\n        <!-- ไว้ใส่รูป -->\r\n        <img\r\n          src=\"https://media.discordapp.net/attachments/953696113448337498/968580127908851712/background.png?width=1191&height=670\"\r\n          alt=\"Placeholder image\"\r\n        />\r\n      </figure>\r\n    </div>\r\n    <section class=\"hero\">\r\n      <div class=\"hero-body\">\r\n        <p class=\"title has-text-white\">กำลังฉาย</p>\r\n        <div class=\"columns\">\r\n          <div class=\"column is-10\">\r\n            <input\r\n              class=\"input is-danger\"\r\n              type=\"text\"\r\n              v-model=\"search\"\r\n              placeholder=\"Search Movie\"\r\n            />\r\n          </div>\r\n          <div class=\"column is-2\">\r\n            <button\r\n              @click=\"getMovie()\"\r\n              class=\"button px-3 is-danger is-fullwidth\"\r\n            >\r\n              <strong>Search</strong>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"field level-right\">\r\n        <div class=\"control\">\r\n          <div class=\"select is-danger\">\r\n            <select v-model=\"theatre\" @change=\"getMovie()\">\r\n            {{theatre}}\r\n              <option value=\"none\">เลือกโรงหนัง</option>\r\n              <option value=\"JunRongJai 1\">JunRongJai 1</option>\r\n              <option value=\"JunRongJai 2\">JunRongJai 2</option>\r\n              <option value=\"RongJai 1\">RongJai 1</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"select ml-5 is-danger is-outlined\">\r\n            <select v-model=\"order\" @change=\"getMovie()\">\r\n              <option value=\"show_date\">เรียงลำดับจาก ล่าสุด</option>\r\n              <option value=\"m_name asc\">เรียงลำดับจาก a-z</option>\r\n              <option value=\"m_name desc\">เรียงลำดับจาก z-a</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"content\">\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n          <p\r\n            v-if=\"movie.length == 0\"\r\n            class=\"title has-text-centered has-text-white\"\r\n          >\r\n            ไม่ค้นพบรายการที่ต้องการ\r\n          </p>\r\n          <div class=\"listmovie column is-3 mt-5\" v-for=\"(movie) in movie\" :key=\"movie.id\" >\r\n            <div class=\"level-left has-text-white\">{{ movie.tname }}</div>\r\n            <div class=\"level-left has-text-white\">{{ movie.show_date }}</div>\r\n            <div class=\"level-left mb-3 has-text-white\">\r\n              {{ movie.st_time }}\r\n            </div>\r\n            <div class=\"card \" style=\" background-color: #a40606; background-color: #99201c;\r\nbackground-image: linear-gradient(#FF7D7D, #ffffff, #FF7D7D);\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 180px; height: 284px; box-shadow: rgba(0, 0, 0, 0.25) 0px 14px 28px, rgba(0, 0, 0, 0.22) 0px 10px 10px;\"\r\n                    :src=\"image(movie.img_movie)\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content has-text-black\" style=\"height: 40px\">\r\n                  {{ movie.m_name }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <footer class=\"card-footer has-background-danger-dark\">\r\n              <router-link\r\n                :to=\"`/show/${movie.show_id}`\"\r\n                class=\"card-footer-item has-text-light\"\r\n              >\r\n                <span v-if=\"isCusto()\">Buy Now</span><span v-if=\"isAdmin()\">Detail</span></router-link\r\n              >\r\n            </footer>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/plugins/axios'\r\nexport default {\r\n  name: \"app\",\r\n  props: ['user'],\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      movie: [],\r\n      order: \"show_date\",\r\n      theatre: \"none\",\r\n    };\r\n  },\r\n  mounted() {\r\n    window.scrollTo(0, 0)\r\n    this.getMovie();\r\n  },\r\n  methods: {\r\n    getMovie() {\r\n      axios\r\n        .get(\"http://localhost:3000/\", {\r\n          params: {\r\n            search: this.search,\r\n            order: this.order,\r\n            theatre: this.theatre,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.movie = response.data;\r\n          for (var i = 0; i < this.movie.length; i++) {\r\n            this.movie[i].show_date = new Date(this.movie[i].show_date).toDateString()\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    image(file_path){\r\n        if (file_path) {\r\n        return \"http://localhost:3000/\" + file_path;\r\n      } else {\r\n        return \"https://bulma.io/images/placeholders/640x360.png\";\r\n      }\r\n    },\r\n    isAdmin(){\r\n      if (!this.user) return false\r\n      return this.user.role == 'admin'\r\n    },\r\n    isCusto(){\r\n      if (!this.user) return false\r\n      return this.user.role == 'customer'\r\n    }\r\n    \r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}