     <p class=\"title has-text-white\">กำลังฉาย</p>\r\n        <div class=\"columns\">\r\n          <div class=\"column is-10\">\r\n            <input\r\n              class=\"input\"\r\n              type=\"text\"\r\n              v-model=\"search\"\r\n              placeholder=\"Search Movie\"\r\n            />\r\n          </div>\r\n          <div class=\"column is-2\">\r\n            <button\r\n              @click=\"getMovie()\"\r\n              class=\"button is-danger px-3 is-fullwidth\"\r\n            >\r\n              <strong>Search</strong>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"field level-right\">\r\n        <div class=\"control\">\r\n          <div class=\"select\">\r\n            <select v-model=\"theatre\" @change=\"getMovie()\">\r\n              <option value=\"none\">เลือกโรงหนัง</option>\r\n              <option value=\"JunRongJai 1\">JunRongJai 1</option>\r\n              <option value=\"JunRongJai 2\">JunRongJai 2</option>\r\n              <option value=\"RongJai 1\">RongJai 1</option>\r\n            </select>\r\n          </div>\r\n          <div class=\"select ml-5\">\r\n            <select v-model=\"order\" @change=\"getMovie()\">\r\n              <option value=\"show_date\">ฉายล่าสุด</option>\r\n              <option value=\"m_name asc\">a-z</option>\r\n              <option value=\"m_name desc\">z-a</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"content\">\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n          <p\r\n            v-if=\"movie.length == 0\"\r\n            class=\"title has-text-centered has-text-white\"\r\n          >\r\n            ไม่ค้นพบรายการที่ต้องการ\r\n          </p>\r\n          <div class=\"column is-3 mt-5\" v-for=\"(movie, index) in movie\" :key=\"movie.id\">\r\n            <div class=\"level-left has-text-white\">{{ movie.tname }}</div>\r\n            <div class=\"level-left has-text-white\">{{ movie.show_date }}</div>\r\n            <div class=\"level-left mb-3 has-text-white\">\r\n              {{ movie.st_time }}\r\n            </div>\r\n            <div class=\"card has-background-warning-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 232px; height: 367px;\"\r\n                    :src=\"image(index)\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 40px\">\r\n                  {{ movie.m_name }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <footer class=\"card-footer has-background-danger-dark\">\r\n              <router-link\r\n                :to=\"`/movie/${movie.show_id}`\"\r\n                class=\"card-footer-item has-text-light\"\r\n              >\r\n                <span>Buy Now</span></router-link\r\n              >\r\n            </footer>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: ['user'],\r\n  data() {\r\n    return {\r\n      order: [],\r\n      customer: [],\r\n      show: [],\r\n      movies: [],\r\n      theatre: [],\r\n      myorder: [],\r\n\r\n      selectSeats: [],\r\n      color: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getOrderCustomer(this.$route.params.cid);\r\n  },\r\n  computed: {\r\n    \r\n  },\r\n  methods: {\r\n    getOrderCustomer(id) {\r\n      axios\r\n        .get(`http://localhost:3000/booking/${id}`)\r\n        .then((response) => {\r\n            this.myorder = response.data.order\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}