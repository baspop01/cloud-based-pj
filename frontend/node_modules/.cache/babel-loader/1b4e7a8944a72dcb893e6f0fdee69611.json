ie","axios","get","params","then","console","catch","image","index"],"sourceRoot":"src/views","sources":["movieDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"container is-widescreen\">\r\n    <div class=\"card-image pt-5\">\r\n      <figure class=\"image is-1186x800\">\r\n        <!-- ไว้ใส่รูป -->\r\n        <img\r\n          src=\"https://media.discordapp.net/attachments/953696113448337498/968580127908851712/background.png?width=1191&height=670\"\r\n          alt=\"Placeholder image\"\r\n        />\r\n      </figure>\r\n    </div>\r\n    <section class=\"hero\">\r\n      <div class=\"hero-body\">\r\n        <p class=\"title has-text-white\">รายการภาพยนตร์ทั้งหมด</p>\r\n        <div class=\"columns\">\r\n          <div class=\"column is-10\">\r\n            <input\r\n              class=\"input\"\r\n              type=\"text\"\r\n              v-model=\"searchdetail\"\r\n              placeholder=\"Searchdetail Movie\"\r\n            />\r\n          </div>\r\n          <div class=\"column is-2\">\r\n            <button class=\"button is-danger px-3 is-fullwidth\">\r\n              <strong>Searchdetail</strong>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"field level-right\">\r\n        <div class=\"control\">\r\n        <button class=\"button is-danger\">\r\n              <strong>Add Movie</strong>\r\n            </button>\r\n          <div class=\"select ml-5\">\r\n            <select v-model=\"sortdetail\" @change=\"getMovie()\">\r\n              <option value=\"m_name asc\">a-z</option>\r\n              <option value=\"m_name desc\">z-a</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"content\">\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n          <div class=\"column is-3 mt-5\" v-for=\"(movie) in movie\" :key=\"movie.m_id\">\r\n            <div class=\"card has-background-warning-light\">\r\n              <div class=\"card-content\">\r\n                <div class=\"content\">\r\n                  <img style=\" width: 232px; height: 367px;\"\r\n                    :src=\"movie.img_movie\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                  {{movie.m_name}}\r\n                  <br />\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <button  class=\"card-footer-item button is-danger\">ดูเพิ่มเติม</button>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '@/plugins/axios'\r\nexport default {\r\n  name: \"app\",\r\n  props: ['user'],\r\n  data() {\r\n    return {\r\n      searchdetail: \"\",\r\n      movie: [],\r\n      sortdetail: \"m_name asc\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getMovie();\r\n  },\r\n  methods: {\r\n    getMovie() {\r\n      axios\r\n        .get(\"http://localhost:3000/movie/detail\", {\r\n          params: {\r\n            searchdetail: this.searchdetail,\r\n            sortdetail: this.sortdetail,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.movie = response.data.movie;\r\n          console.log(response)\r\n          // for (var i = 0; i < this.movie.length; i++) {\r\n          //   var date = this.movie[i].show_date.split(\"T\")[0];\r\n          //   date = date.split(\"-\");\r\n          //   this.movie[i].show_date = date[0] + \"-\" + date[1] + \"-\" + (parseInt(date[2]) + 1);\r\n          // }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    image(index){\r\n      return this.movie[index].img_movie\r\n    },\r\n    \r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}