y=\"r\">\r\n            <td v-for=\"c in cols\" class=\"pl-2\" style=\"width: 50px\" :key=\"c\">\r\n              <div class=\"has-text-white\">{{ r }}{{ c }}</div>\r\n              <div v-if=\"\" class=\"has-text-white\">{{ r }}{{ c }}</div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <div class=\"columns\" v-for=\"seat in seat\" :key=\"seat.seat_id\"></div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  data() {\r\n    return {\r\n      seat: [],\r\n      movie: [],\r\n      seatBook: [],\r\n      rows: [],\r\n      cols: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getSeat(this.$route.params.id);\r\n  },\r\n  methods: {\r\n    getSeat(id) {\r\n      axios\r\n        .get(`http://localhost:3000/movie/${id}`)\r\n        .then((response) => {\r\n          this.seat = response.data.seat;\r\n          this.movie = response.data.movie[0];\r\n          this.seatBook = response.data.seatBook;\r\n          var date = this.movie.show_date.split(\"T\")[0];\r\n          console.log(\"A01\" == this.seatBook.no_of_seats[0])\r\n          this.movie.show_date = date;\r\n          if(this.seat[0].THEATRE_tid == 1 || this.seat[0].THEATRE_tid == 2){\r\n            this.rows = ['A', 'B', 'C', 'D', 'E']\r\n            this.cols = ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10']\r\n          }else if (this.seat[0].THEATRE_tid == 3){\r\n            this.rows = ['A', 'B', 'C']\r\n            this.cols = ['01', '02', '03', '04', '05']\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}