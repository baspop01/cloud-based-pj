e(\"http://localhost:3000/movie/cancel/\".concat(id)).then(function (res) {\n        _this2.cancleModal = false;\n\n        _this2.getOrderCustomer(_this2.$route.params.id);\n\n        console.log(res);\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    linkOrder: function linkOrder(id) {\n      this.$router.push({\n        path: \"/booking/\".concat(id)\n      });\n    },\n    getTicket: function getTicket(id) {\n      var _this3 = this;\n\n      this.ticketModal = true;\n      axios.get(\"http://localhost:3000/profile/myorder/ticket/\".concat(id)).then(function (res) {\n        _this3.ticket = res.data.ticket;\n\n        for (var j = 0; j < _this3.ticket.length; j++) {\n          var dateTicket = _this3.ticket[j].show_date.split(\"T\")[0];\n\n          dateTicket = dateTicket.split(\"-\");\n          _this3.ticket[j].show_date = dateTicket[1] + \"/\" + (parseInt(dateTicket[2]) + 1);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyHA;AACA;AACAA,aADA;AAEAC,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,kBADA;AAEAC,eAFA;AAGAC,cAHA;AAIAC,cAJA;AAKAC,gBALA;AAMAC,wBANA;AAOAC;AAPA;AASA,GAbA;AAcAC,SAdA,qBAcA;AACA;AACA,GAhBA;AAiBAC,cAjBA;AAkBAC;AACAC,oBADA,4BACAC,EADA,EACA;AAAA;;AACAC;AACA;AACA;AACA;;AAEA;AACA;;AACAC;AACA;AACA;;AACA;AACA;;AACAC;AACA;AACA;AACA,OAfA;AAgBA,KAlBA;AAmBAC,iBAnBA,yBAmBAJ,EAnBA,EAmBA;AAAA;;AACAC,YACAI,MADA,8CACAL,EADA,GAEAM,IAFA,CAEA;AACA;;AACA;;AACAC;AACA,OANA,EAMAC,KANA,CAMA;AACAD;AACA,OARA;AASA,KA7BA;AA8BAE,aA9BA,qBA8BAT,EA9BA,EA8BA;AACA;AAAAU;AAAA;AACA,KAhCA;AAiCAC,aAjCA,qBAiCAX,EAjCA,EAiCA;AAAA;;AACA;AACAC,4EACAK,IADA,CACA;AACA;;AACA;AACA;;AACAM;AACA;AACA;AACA,OARA;AASA;AA5CA;AAlBA","names":["name","props","data","customer","order","paid","wait","ticket","cancleModal","ticketModal","mounted","computed","methods","getOrderCustomer","id","axios","datePaid","dateWait","confirmDelete","delete","then","console","catch","linkOrder","path","getTicket","dateTicket"],"sourceRoot":"src/views","sources":["Order.vue"],"sourcesContent":["<template>\r\n  <div class=\"container is-widescreen\">\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"content\">\r\n        <p class=\"title has-text-left has-text-white mb-2\">\r\n          My Order ({{ order.length }})\r\n        </p>\r\n        <hr />\r\n        <h1 class=\"is-size-4 has-text-left has-text-white mb-6\">\r\n          ยังไม่ได้ชำระเงิน ({{wait.length}})\r\n        </h1>\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n                              <p v-if=\"wait.length == 0\" class=\"ml-5 has-text-white\" >\r\n            ไม่มีรายการคำสั่งซื้อ\r\n          </p>\r\n          <div class=\"column is-3 mt-5\" v-for=\"wait in wait\" :key=\"wait\">\r\n            <div class=\"card has-background-danger-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 150px; height: 237px;\"\r\n                    :src=\"wait.img_movie\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 100px\">\r\n                  รายการสั่งซื้อที่: {{wait.order_id}}<br />\r\n                  {{wait.m_name}} ({{wait.language}})<br />\r\n                  วันที่ฉาย: {{wait.show_date}}\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer \">\r\n                <button @click=\"linkOrder(wait.order_id)\" class=\"card-footer-item  button is-success\">ชำระเงิน</button>\r\n                <button  class=\"card-footer-item  button is-danger\" @click=\"cancleModal = true\">ยกเลิก</button>\r\n              </footer>\r\n            </div>\r\n          <div class=\"modal\" :class=\"{'is-active':  cancleModal}\">\r\n            <div class=\"modal-background\"></div>\r\n              <div class=\"modal-card\">\r\n                <header class=\"modal-card-head\">\r\n                    <p class=\"modal-card-title\"><strong>Are you sure want to delete รายการสั่งซื้อที่ {{wait.order_id}}?</strong></p>\r\n                </header>\r\n                <footer class=\"modal-card-foot\">\r\n                    <button @click=\"confirmDelete(wait.order_id)\" class=\"button is-danger\">Yes</button>\r\n                  <button @click=\"cancleModal = false\" class=\"button is-success\">No</button>\r\n                </footer>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <hr class=\"mt-6\" />\r\n        <h1 class=\"is-size-4 has-text-left has-text-white mb-6\">\r\n          ชำระเงินแล้ว ({{paid.length}})\r\n        </h1>\r\n        <div class=\"columns is-multiline\">\r\n          <p v-if=\"paid.length == 0\" class=\"ml-5 has-text-white\" >\r\n            ไม่มีรายการชำระเงิน\r\n          </p>\r\n          <!-- loop -->\r\n\r\n          <div class=\"column is-3 mt-5\" v-for=\"paid in paid\" :key=\"paid.order_id\">\r\n            <div class=\"card has-background-primary-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 150px; height: 237px;\"\r\n                    :src=\"paid.img_movie\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 100px\">\r\n                  รายการสั่งซื้อที่: {{paid.order_id}}<br />\r\n                  {{paid.m_name}}  ({{paid.language}})<br />\r\n                  วันที่ฉาย: {{paid.show_date}}\r\n                </div>\r\n              </div>\r\n                        <div class=\"modal\" :class=\"{'is-active':  ticketModal}\">\r\n            <div class=\"modal-background\"></div>\r\n              <div class=\"modal-card\">\r\n                <header class=\"modal-card-head\">\r\n                    <p class=\"modal-card-title\"><strong>รายการคำสั่งซื้อที่ {{paid.order_id}}</strong></p>\r\n                </header>\r\n                  <div class=\"modal-content has-background-white\">\r\n                    <div class=\"card\">\r\n                      <div class=\"card-content\" v-for=\"ticket in ticket\" :key=\"ticket\">\r\n                        <div class=\"media\">\r\n                          <div class=\"media-left\">\r\n                            <figure class=\"image is-96x96\">\r\n                              <img :src=\"ticket.img_movie\" alt=\"Placeholder image\">\r\n                            </figure>\r\n                          </div>\r\n                          <div class=\"media-content has-text-left\">\r\n                            <p class=\"title is-4 is-size-6\">{{ticket.m_name}} ({{ticket.language}})</p><br>\r\n                            <p class=\"subtitle is-6\">{{ticket.show_date}}</p>\r\n                          </div>\r\n                        </div>\r\n                        <br><br>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  <footer class=\"modal-card-foot\">\r\n                    <button @click=\"ticketModal = false\" class=\"button is-success\">ออก</button>\r\n                  </footer>\r\n              </div>\r\n            </div>\r\n              <footer class=\"card-footer\">\r\n                <button  @click=\"getTicket(paid.order_id)\" class=\"card-footer-item button is-info\">รายละเอียดตั๋ว</button>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"@/plugins/axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: [\"user\"],\r\n  data() {\r\n    return {\r\n      customer: [],\r\n      order: [],\r\n      paid: [],\r\n      wait: [],\r\n      ticket: [],\r\n      cancleModal: false,\r\n      ticketModal: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getOrderCustomer(this.$route.params.id);\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    getOrderCustomer(id) {\r\n      axios.get(`http://localhost:3000/profile/myorder/${id}`).then((res) => {\r\n        this.order = res.data.order;\r\n        this.paid = res.data.paid;\r\n        this.wait = res.data.wait;\r\n\r\n        for (var i = 0; i < this.paid.length; i++){\r\n          var datePaid = this.paid[i].show_date.split(\"T\")[0];\r\n          datePaid = datePaid.split(\"-\");\r\n          this.paid[i].show_date = datePaid[0] + \"-\" + datePaid[1] + \"-\" + (parseInt(datePaid[2]) + 1);\r\n        }\r\n        for (var j = 0; j < this.wait.length; j++){\r\n          var dateWait = this.wait[j].show_date.split(\"T\")[0];\r\n          dateWait = dateWait.split(\"-\");\r\n          this.wait[j].show_date = dateWait[0] + \"-\" + dateWait[1] + \"-\" + (parseInt(dateWait[2]) + 1);\r\n        }\r\n      });\r\n    },\r\n    confirmDelete(id){\r\n            axios\r\n      .delete(`http://localhost:3000/movie/cancel/${id}`)\r\n      .then((res) => {\r\n        this.cancleModal = false\r\n        this.getOrderCustomer(this.$route.params.id);\r\n        console.log(res)\r\n      }).catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    linkOrder(id){\r\n      this.$router.push({path: `/booking/${id}`})\r\n    },\r\n    getTicket(id){\r\n      this.ticketModal = true\r\n      axios.get(`http://localhost:3000/profile/myorder/ticket/${id}`)\r\n      .then((res) => {\r\n        this.ticket = res.data.ticket\r\n        for (var j = 0; j < this.ticket.length; j++){\r\n          var dateTicket = this.ticket[j].show_date.split(\"T\")[0];\r\n          dateTicket = dateTicket.split(\"-\");\r\n          this.ticket[j].show_date = dateTicket[1] + \"/\" + (parseInt(dateTicket[2]) + 1);\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}