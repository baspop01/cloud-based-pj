{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.search.js\";\nimport \"core-js/modules/es.string.split.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: \"app\",\n  data: function data() {\n    return {\n      search: \"\",\n      movie: [],\n      order: \"latest\"\n    };\n  },\n  mounted: function mounted() {\n    this.getMovie();\n  },\n  methods: {\n    getMovie: function getMovie() {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/\", {\n        params: {\n          search: this.search,\n          order: this.order\n        }\n      }).then(function (response) {\n        _this.movie = response.data;\n\n        for (var i = 0; i < _this.movie.length; i++) {\n          var date = _this.movie[i].show_date.split(\"T\")[0];\n\n          _this.movie[i].show_date = date;\n        }\n\n        console.log(response.data);\n      }).catch(function (err) {\n        console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmEA;AACA;AACAA,aADA;AAEAC,MAFA,kBAEA;AACA;AACAC,gBADA;AAEAC,eAFA;AAGAC;AAHA;AAKA,GARA;AASAC,SATA,qBASA;AACA;AACA,GAXA;AAYAC;AACAC,YADA,sBACA;AAAA;;AACAC,YACAC,GADA,CACA,wBADA,EACA;AACAC;AACAR,6BADA;AAEAE;AAFA;AADA,OADA,EAOAO,IAPA,CAOA;AACA;;AACA;AACA;;AACA;AACA;;AACAC;AACA,OAdA,EAeAC,KAfA,CAeA;AACAD;AACA,OAjBA;AAkBA;AApBA;AAZA","names":["name","data","search","movie","order","mounted","methods","getMovie","axios","get","params","then","console","catch"],"sourceRoot":"src/views","sources":["Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"container is-widescreen\">\r\n    <section class=\"hero\">\r\n      <div class=\"hero-body\">\r\n        <p class=\"title has-text-white\">กำลังฉาย</p>\r\n        <div class=\"columns\">\r\n          <div class=\"column is-10\">\r\n            <input\r\n              class=\"input\"\r\n              type=\"text\"\r\n              v-model=\"search\"\r\n              placeholder=\"Search Movie\"\r\n            />\r\n          </div>\r\n          <div class=\"column is-3\">\r\n            <button @click=\"getMovie()\" class=\"button\">Search</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"field level-right\">\r\n        <div class=\"control\">\r\n          <div class=\"select\">\r\n            <select v-model=\"order\" @change=\"getMovie()\">\r\n              <option value=\"latest\">ฉายล่าสุด</option>\r\n              <option value=\"abc\">a-z</option>\r\n              <option value=\"cba\">z-a</option>\r\n            </select>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"content\">\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n          <div class=\"column is-3\" v-for=\"movie in movie\" :key=\"movie.id\">\r\n            <div class=\"level-left has-text-white\">{{ movie.show_date }}</div>\r\n            <div class=\"level-left mb-3 has-text-white\">\r\n              {{ movie.st_time }}\r\n            </div>\r\n            <div class=\"card has-background-warning-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure class=\"image is-1186x800\">\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img\r\n                    src=\"https://s359.kapook.com/pagebuilder/cf0dd339-b4a5-49ef-8332-25d490e03da8.jpg\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 40px\">\r\n                  {{ movie.m_name }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <footer class=\"card-footer has-background-danger-dark\">\r\n              <router-link :to=\"`/movie/${movie.show_id}`\" class=\"card-footer-item has-text-light\"> <span>Buy Now</span></router-link>\r\n            </footer>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  data() {\r\n    return {\r\n      search: \"\",\r\n      movie: [],\r\n      order: \"latest\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getMovie();\r\n  },\r\n  methods: {\r\n    getMovie() {\r\n      axios\r\n        .get(\"http://localhost:3000/\", {\r\n          params: {\r\n            search: this.search,\r\n            order: this.order,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          this.movie = response.data;\r\n          for (var i = 0; i < this.movie.length; i++) {\r\n            var date = this.movie[i].show_date.split(\"T\")[0];\r\n            this.movie[i].show_date = date;\r\n          }\r\n          console.log(response.data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}