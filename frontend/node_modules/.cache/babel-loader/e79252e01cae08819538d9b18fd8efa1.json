\n      <div class=\"hero-body\">\r\n        <p class=\"title\">Movie Theatre</p>\r\n        <div class=\"columns\">\r\n          <div class=\"column is-half\">\r\n            <input\r\n              class=\"input\"\r\n              type=\"text\"\r\n              placeholder=\"Search blog(s)\"\r\n            />\r\n          </div>\r\n          <div class=\"column is-half\">\r\n            <button class=\"button\">Search</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"content\">\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n          <div class=\"column is-3\" v-for=\"movie in movie\" :key=\"movie.id\">\r\n            <div class=\"card\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure class=\"image\">\r\n                  <!-- ไว้ใส่รูป -->\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"title\">{{ movie.m_name }}</div>\r\n                <div class=\"content\" style=\"height: 200px\"></div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <router-link class=\"card-footer-item\">Read more...</router-link>\r\n                <a class=\"card-footer-item\">\r\n                  <span class=\"icon-text\">\r\n                    <span class=\"icon\">\r\n                      <i class=\"far fa-heart\"></i>\r\n                    </span>\r\n                    <span>Like</span>\r\n                  </span>\r\n                </a>\r\n                <a class=\"card-footer-item\">\r\n                  <span class=\"icon-text\">\r\n                    <span>Edit</span>\r\n                  </span>\r\n                </a>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: {\r\n    msg: String,\r\n  },\r\n  mouthed() {\r\n    this.getMovie();\r\n  },\r\n  data() {\r\n    return {\r\n      movies: [\r\n        { mid: 1, name: \"spiderman\" },\r\n        { mid: 2, name: \"starwars\" },\r\n        { mid: 3, name: \"helloworld\" },\r\n        { mid: 4, name: \"yourmom\" },\r\n        { mid: 5, name: \"gundum\" },\r\n        { mid: 6, name: \"kitty\" },\r\n      ],\r\n      movie: [],\r\n    };\r\n  },\r\n  method: {\r\n    getMovie() {\r\n        console.log(\"คิด\")\r\n      axios\r\n        .get(\"http://localhost:3000/\")\r\n        .then((response) => {\r\n          this.movie = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}