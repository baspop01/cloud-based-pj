{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"@/plugins/axios\";\nexport default {\n  name: \"app\",\n  props: [\"user\"],\n  data: function data() {\n    return {\n      movie: [],\n      show: [],\n      editShow: false\n    };\n  },\n  mounted: function mounted() {\n    window.scrollTo(0, 0);\n    this.getMovieDetail(this.$route.params.id);\n  },\n  methods: {\n    getMovieDetail: function getMovieDetail(id) {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/movies/detail/\".concat(id)).then(function (response) {\n        _this.movie = response.data.moviedetail[0];\n        _this.show = response.data.show;\n        _this.movie.release_date = new Date(_this.movie.release_date).toDateString();\n\n        for (var i = 0; i < _this.show.length; i++) {\n          _this.show[i].show_date = new Date(_this.show[i].show_date).toDateString();\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    image: function image(file_path) {\n      if (file_path) {\n        return \"http://localhost:3000/\" + file_path;\n      } else {\n        return \"https://bulma.io/images/placeholders/640x360.png\";\n      }\n    },\n    isAdmin: function isAdmin() {\n      if (!this.user) return false;\n      return this.user.role == 'admin';\n    },\n    isCusto: function isCusto() {\n      if (!this.user) return false;\n      return this.user.role == 'customer';\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAgGA;AACA;AACAA,aADA;AAEAC,iBAFA;AAGAC,MAHA,kBAGA;AACA;AACAC,eADA;AAEAC,cAFA;AAGAC;AAHA;AAKA,GATA;AAUAC,SAVA,qBAUA;AACAC;AACA;AACA,GAbA;AAcAC;AACAC,kBADA,0BACAC,EADA,EACA;AAAA;;AACAC,YACAC,GADA,+CACAF,EADA,GAEAG,IAFA,CAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA,OAVA,EAWAC,KAXA,CAWA;AACAC;AACA,OAbA;AAcA,KAhBA;AAiBAC,SAjBA,iBAiBAC,SAjBA,EAiBA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AACA,KAvBA;AAwBAC,WAxBA,qBAwBA;AACA;AACA;AACA,KA3BA;AA4BAC,WA5BA,qBA4BA;AACA;AACA;AACA;AA/BA;AAdA","names":["name","props","data","movie","show","editShow","mounted","window","methods","getMovieDetail","id","axios","get","then","catch","console","image","file_path","isAdmin","isCusto"],"sourceRoot":"src/views","sources":["movieDetail.vue"],"sourcesContent":["<template>\r\n  <section class=\"section\" id=\"app\">\r\n    <div class=\"content\">\r\n      <div class=\"columns\">\r\n        <div class=\"column\">\r\n          <img\r\n            style=\"\r\n              width: 432px;\r\n              height: 587px;\r\n              margin-left: 15%;\r\n              margin-right: 15%;\r\n            \"\r\n            :src=\"image(movie.img_movie)\"\r\n          />\r\n        </div>\r\n        <div class=\"column is-6 level-right\">\r\n          <div class=\"title\">\r\n            <p class=\"title has-text-white\">{{ movie.m_name }}</p>\r\n          </div>\r\n          <hr />\r\n          <div class=\"detail\">\r\n            <p class=\"has-text-white has-text-justified\">\r\n              {{ movie.plot }}\r\n            </p>\r\n            <table\r\n              class=\"table is-bordered has-background-black has-text-white\"\r\n            >\r\n              <tr>\r\n                <td>Release Date</td>\r\n                <td>{{ movie.release_date }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Director</td>\r\n                <td>{{ movie.director }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Cast</td>\r\n                <td>\r\n                  {{ movie.actors }}\r\n                </td>\r\n              </tr>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"column m-5\">\r\n        <p class=\"title has-text-white\">รอบฉาย</p>\r\n        <div class=\"field level-right\">\r\n          <div class=\"control\">\r\n          <router-link :to=\"`/AddShow/${movie.m_id}`\" v-if=\"isAdmin()\">\r\n            <button class=\"button is-warning\">\r\n              <strong>Add Show</strong>\r\n            </button>\r\n            </router-link>\r\n            <button class=\"button is-danger ml-5\" >\r\n              <strong>Edit Show</strong>\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <table class=\"table is-bordered has-background-danger-dark has-text-white\">\r\n          <tr>\r\n            <td>Theatre</td>\r\n            <td>Date</td>\r\n            <td>Start</td>\r\n            <td>End</td>\r\n            <td>Language</td>\r\n            <td>Link</td>\r\n          </tr>\r\n          <tr v-for=\"show in show\" :key=\"show.show_id \" class=\"has-background-danger-light has-text-black\">\r\n            <td>{{ show.tname }}</td>\r\n            <td>{{ show.show_date }}</td>\r\n            <td>{{ show.st_time }}</td>\r\n            <td>{{ show.end_time }}</td>\r\n            <td>{{ show.language }}</td>\r\n            <td>\r\n              <router-link :to=\"`/show/${show.show_id}`\"\r\n                ><button class=\"button is-danger\">\r\n                  <span>Click</span>\r\n                </button></router-link\r\n              >\r\n            </td>\r\n          </tr>\r\n          <tr v-if=\"show.length == 0\" class=\"has-background-danger-light has-text-black\">\r\n            <td>-</td>\r\n            <td>-</td>\r\n            <td>-</td>\r\n            <td>-</td>\r\n            <td>-</td>\r\n            <td>-</td>\r\n          </tr>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n<script>\r\nimport axios from \"@/plugins/axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: [\"user\"],\r\n  data() {\r\n    return {\r\n      movie: [],\r\n      show: [],\r\n      editShow: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    window.scrollTo(0, 0)\r\n    this.getMovieDetail(this.$route.params.id);\r\n  },\r\n  methods: {\r\n    getMovieDetail(id) {\r\n      axios\r\n        .get(`http://localhost:3000/movies/detail/${id}`)\r\n        .then((response) => {\r\n          this.movie = response.data.moviedetail[0];\r\n          this.show = response.data.show;\r\n          this.movie.release_date = new Date(this.movie.release_date).toDateString()\r\n  \r\n          for (var i = 0; i < this.show.length; i++) {\r\n            this.show[i].show_date = new Date(this.show[i].show_date).toDateString()\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    image(file_path){\r\n        if (file_path) {\r\n        return \"http://localhost:3000/\" + file_path;\r\n      } else {\r\n        return \"https://bulma.io/images/placeholders/640x360.png\";\r\n      }\r\n    },\r\n    isAdmin(){\r\n      if (!this.user) return false\r\n      return this.user.role == 'admin'\r\n    },\r\n    isCusto(){\r\n      if (!this.user) return false\r\n      return this.user.role == 'customer'\r\n    }\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}