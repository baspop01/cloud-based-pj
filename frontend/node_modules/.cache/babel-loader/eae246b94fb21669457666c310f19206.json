\"container is-widescreen\">\r\n    <section class=\"section\" id=\"app\">\r\n      <div class=\"content\">\r\n        <p class=\"title has-text-left has-text-white mb-2\">\r\n          My Order ({{ order.length }})\r\n        </p>\r\n        <hr />\r\n        <h1 class=\"is-size-4 has-text-left has-text-white mb-6\">\r\n          ยังไม่ได้ชำระเงิน({{wait.length}})\r\n        </h1>\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n\r\n          <div class=\"column is-3 mt-5\" v-for=\"wait in wait\" :key=\"wait\">\r\n            <div class=\"card has-background-danger-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 150px; height: 237px;\"\r\n                    :src=\"wait.img_movie\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 100px\">\r\n                  รายการที่: {{wait.order_id}}<br />\r\n                  เรื่อง: {{wait.m_name}}<br />\r\n                  วันที่ฉาย: {{wait.show_date}}\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer \">\r\n              <router-link :to=\"saf\"><button class=\"card-footer-item  button is-success\">ชำระเงิน</button></router-link>\r\n                <button  class=\"card-footer-item  button is-danger\" @click=\"closeModal()\">ยกเลิก</button>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <hr class=\"mt-6\" />\r\n        <h1 class=\"is-size-4 has-text-left has-text-white mb-6\">\r\n          ชำระเงินแล้ว({{paid.length}})\r\n        </h1>\r\n        <div class=\"columns is-multiline\">\r\n          <!-- loop -->\r\n\r\n          <div class=\"column is-3 mt-5\" v-for=\"paid in paid\" :key=\"paid\">\r\n            <div class=\"card has-background-primary-light\">\r\n              <div class=\"card-image pt-5\">\r\n                <figure>\r\n                  <!-- ไว้ใส่รูป -->\r\n                  <img style=\" width: 150px; height: 237px;\"\r\n                    :src=\"paid.img_movie\"\r\n                    alt=\"Placeholder image\"\r\n                  />\r\n                </figure>\r\n              </div>\r\n              <div class=\"card-content\">\r\n                <div class=\"content\" style=\"height: 100px\">\r\n                  รายการที่: {{paid.order_id}}<br />\r\n                  เรื่อง: {{paid.m_name}}<br />\r\n                  วันที่ฉาย: {{paid.show_date}}\r\n                </div>\r\n              </div>\r\n              <footer class=\"card-footer\">\r\n                <button class=\"card-footer-item button is-info\">รายละเอียดตั๋ว</button>\r\n              </footer>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"@/plugins/axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: [\"user\"],\r\n  data() {\r\n    return {\r\n      customer: [],\r\n      order: [],\r\n      paid: [],\r\n      wait: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getOrderCustomer(this.$route.params.id);\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    getOrderCustomer(id) {\r\n      axios.get(`http://localhost:3000/profile/myorder/${id}`).then((res) => {\r\n        console.log(res);\r\n        this.order = res.data.order;\r\n        this.paid = res.data.paid;\r\n        this.wait = res.data.wait;\r\n      });\r\n    },\r\n    closeModal(){\r\n      axios\r\n      .delete(`http://localhost:3000/movie/cancel/${this.order.order_id}`)\r\n      .then((res) => {\r\n        res\r\n      }).catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}