{"ast":null,"code":"import \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.split.js\";\nimport \"core-js/modules/es.array.includes.js\";\nimport \"core-js/modules/es.string.includes.js\";\nimport \"core-js/modules/es.array.splice.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport axios from \"axios\";\nexport default {\n  name: \"app\",\n  data: function data() {\n    return {\n      seat: [],\n      movie: [],\n      seatBook: [],\n      rows: [],\n      cols: [],\n      selectSeats: [],\n      color: null\n    };\n  },\n  mounted: function mounted() {\n    this.getSeat(this.$route.params.id);\n  },\n  methods: {\n    getSeat: function getSeat(id) {\n      var _this = this;\n\n      axios.get(\"http://localhost:3000/movie/\".concat(id)).then(function (response) {\n        _this.seat = response.data.seat;\n        _this.movie = response.data.movie[0];\n\n        _this.getSeatBook(response.data.seatBook);\n\n        var date = _this.movie.show_date.split(\"T\")[0];\n\n        date = date.split(\"-\");\n        _this.movie.show_date = date[0] + \"-\" + date[1] + \"-\" + (parseInt(date[2]) + 1);\n\n        if (_this.seat[0].THEATRE_tid == 1 || _this.seat[0].THEATRE_tid == 2) {\n          _this.rows = [\"A\", \"B\", \"C\", \"D\", \"E\"];\n          _this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\", \"06\", \"07\", \"08\", \"09\", \"10\"];\n        } else if (_this.seat[0].THEATRE_tid == 3) {\n          _this.rows = [\"A\", \"B\", \"C\"];\n          _this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\"];\n        }\n      }).catch(function (err) {\n        console.log(err);\n      });\n    },\n    getSeatBook: function getSeatBook(Book) {\n      for (var i = 0; i < Book.length; i++) {\n        this.seatBook.push(Book[i].no_of_seats);\n      }\n    },\n    selectSeat: function selectSeat(r, c) {\n      if (this.selectSeats.includes(r + c)) {\n        this.selectSeats.splice(this.selectSeats.indexOf(r + c), 1);\n      } else {\n        this.selectSeats.push(r + c);\n      }\n    },\n    getType: function getType(r, c) {\n      for (var i = 0; i < this.seat.length; i++) {\n        if (this.seat[i].no_of_seats.includes(r + c)) {\n          var type = i;\n        }\n      }\n\n      console.log(type);\n\n      if (this.seat[type].type_id == '1') {\n        return 'Normal';\n      } else if (this.seat[type].type_id == '2') {\n        return 'Honeymoon';\n      } else if (this.seat[type].type_id == '3') {\n        return 'First Class';\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8FA;AACA;AACAA,aADA;AAEAC,MAFA,kBAEA;AACA;AACAC,cADA;AAEAC,eAFA;AAGAC,kBAHA;AAIAC,cAJA;AAKAC,cALA;AAMAC,qBANA;AAOAC;AAPA;AASA,GAZA;AAaAC,SAbA,qBAaA;AACA;AACA,GAfA;AAgBAC;AACAC,WADA,mBACAC,EADA,EACA;AAAA;;AACAC,YACAC,GADA,uCACAF,EADA,GAEAG,IAFA,CAEA;AACA;AACA;;AACA;;AACA;;AACAC;AACA,gCACAA,uDADA;;AAEA;AACA;AACA;AACA,SAHA,MAGA;AACA;AACA;AACA;AACA,OAjBA,EAkBAC,KAlBA,CAkBA;AACAC;AACA,OApBA;AAqBA,KAvBA;AAwBAC,eAxBA,uBAwBAC,IAxBA,EAwBA;AACA;AACA;AACA;AACA,KA5BA;AA6BAC,cA7BA,sBA6BAC,CA7BA,EA6BAC,CA7BA,EA6BA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AACA,KAnCA;AAoCAC,WApCA,mBAoCAF,CApCA,EAoCAC,CApCA,EAoCA;AACA;AACA;AACA;AACA;AACA;;AACAL;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA,OAFA,MAEA;AACA;AACA;AACA;AAlDA;AAhBA","names":["name","data","seat","movie","seatBook","rows","cols","selectSeats","color","mounted","methods","getSeat","id","axios","get","then","date","catch","console","getSeatBook","Book","selectSeat","r","c","getType"],"sourceRoot":"src/views","sources":["movie.vue"],"sourcesContent":["<template>\r\n  <section class=\"section\" id=\"app\">\r\n    <div class=\"content\">\r\n      <div class=\"columns\">\r\n        <div class=\"column\">\r\n          <img\r\n            style=\"\r\n              width: 232px;\r\n              height: 367px;\r\n              margin-left: 15%;\r\n              margin-right: 15%;\r\n            \"\r\n            :src=\"movie.img_movie\"\r\n          />\r\n        </div>\r\n        <div class=\"column is-6 level-right\">\r\n          <div class=\"title\">\r\n            <p class=\"title has-text-white\">{{ movie.m_name }}</p>\r\n          </div>\r\n          <hr />\r\n          <div class=\"detail\">\r\n            <p class=\"has-text-white has-text-justified\">\r\n              {{ movie.plot }}\r\n            </p>\r\n            <table\r\n              class=\"table is-bordered has-background-black has-text-white\"\r\n            >\r\n              <tr>\r\n                <td>Theatre</td>\r\n                <td>{{ movie.tname }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Date</td>\r\n                <td>{{ movie.show_date }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Time</td>\r\n                <td>{{ movie.st_time }} - {{ movie.end_time }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Director</td>\r\n                <td>{{ movie.director }}</td>\r\n              </tr>\r\n              <tr>\r\n                <td>Cast</td>\r\n                <td>\r\n                  {{ movie.actors }}\r\n                </td>\r\n              </tr>\r\n            </table>\r\n          </div>\r\n        </div>\r\n        \r\n      </div>\r\n      <div class=\"columns m-5 is-multiline\">\r\n        <div class=\"column is-8\"> \r\n          <table class=\"mx-auto\" style=\"width: 80%\">\r\n            <tbody>\r\n              <tr v-for=\"r in rows\" :key=\"r\">\r\n                <td v-for=\"c in cols\" class=\"pl-2\" :key=\"c\">\r\n                  <div class=\"has-background-grey has-text-white\" v-if=\"seatBook.includes(r + c)\">\r\n                    {{ r }}{{ c }}\r\n                  </div>\r\n                  <a @click=\"selectSeat(r,c)\" v-else-if=\"selectSeats.includes(r + c)\" ><div class=\"has-background-warning has-text-dark\">\r\n                    {{ r }}{{ c }}\r\n                  </div></a>\r\n                  <a v-else-if=\"getType(r,c) == 'Normal'\" @click=\"selectSeat(r,c)\" ><div class='has-text-white has-background-danger-dark'>\r\n                    {{ r }}{{ c }}\r\n                  </div></a>\r\n                  <a v-else-if=\"getType(r,c) == 'Honeymoon'\" @click=\"selectSeat(r,c)\" ><div class='has-text-white has-background-info'>\r\n                    {{ r }}{{ c }}\r\n                  </div></a>\r\n                  <a v-else-if=\"getType(r,c) == 'First Class'\" @click=\"selectSeat(r,c)\" ><div class='has-text-white has-background-primary'>\r\n                    {{ r }}{{ c }}\r\n                  </div></a>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n        <div class=\"column is-4 \">\r\n                    <article class=\"panel is-danger has-background-white\">\r\n            <p class=\"panel-heading\">\r\n              All Order\r\n            </p>\r\n            <p>Price: 100B</p>\r\n          </article>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"app\",\r\n  data() {\r\n    return {\r\n      seat: [],\r\n      movie: [],\r\n      seatBook: [],\r\n      rows: [],\r\n      cols: [],\r\n      selectSeats: [],\r\n      color: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getSeat(this.$route.params.id);\r\n  },\r\n  methods: {\r\n    getSeat(id) {\r\n      axios\r\n        .get(`http://localhost:3000/movie/${id}`)\r\n        .then((response) => {\r\n          this.seat = response.data.seat;\r\n          this.movie = response.data.movie[0];\r\n          this.getSeatBook(response.data.seatBook);\r\n          var date = this.movie.show_date.split(\"T\")[0];\r\n          date = date.split(\"-\");\r\n          this.movie.show_date =\r\n            date[0] + \"-\" + date[1] + \"-\" + (parseInt(date[2]) + 1);\r\n          if (this.seat[0].THEATRE_tid == 1 || this.seat[0].THEATRE_tid == 2) {\r\n            this.rows = [\"A\", \"B\", \"C\", \"D\", \"E\"];\r\n            this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\", \"06\", \"07\", \"08\", \"09\", \"10\"];\r\n          } else if (this.seat[0].THEATRE_tid == 3) {\r\n            this.rows = [\"A\", \"B\", \"C\"];\r\n            this.cols = [\"01\", \"02\", \"03\", \"04\", \"05\"];\r\n          }\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n    getSeatBook(Book) {\r\n      for (var i = 0; i < Book.length; i++) {\r\n        this.seatBook.push(Book[i].no_of_seats);\r\n      }\r\n    },\r\n    selectSeat(r, c){\r\n      if (this.selectSeats.includes(r+c)){\r\n        this.selectSeats.splice(this.selectSeats.indexOf(r+c), 1)\r\n      }else{\r\n        this.selectSeats.push(r+c);\r\n      }\r\n    },\r\n    getType(r, c){\r\n      for (var i = 0; i < this.seat.length; i++) {\r\n        if (this.seat[i].no_of_seats.includes(r+c)){\r\n          var type = i\r\n        }\r\n      }\r\n      console.log(type)\r\n      if (this.seat[type].type_id == '1'){\r\n        return 'Normal'\r\n      }else if (this.seat[type].type_id == '2'){\r\n        return 'Honeymoon'\r\n      }else if (this.seat[type].type_id == '3'){\r\n        return 'First Class'\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}