n          </div>\r\n  </section>\r\n</template>\r\n<script>\r\nimport axios from \"@/plugins/axios\";\r\nexport default {\r\n  name: \"app\",\r\n  props: [\"user\"],\r\n  data() {\r\n    return {\r\n        customer: '',\r\n        customerModal: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    window.scrollTo(0, 0);\r\n    this.getCustomer();\r\n  },\r\n  methods: {\r\n      getCustomer(){\r\n          axios\r\n        .get(`http://localhost:3000/customer/`)\r\n        .then((response) => {\r\n        console.log(response.data.customer)\r\n          this.customer = response.data.customer\r\n        })\r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n      }\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}